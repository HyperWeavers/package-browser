  },
  "dependencies": {
    "@angular/common": "~4.3.1",
    "@angular/compiler": "~4.3.1",
    "@angular/core": "~4.3.1",
    "@angular/platform-browser": "~4.3.1",
    "@angular/platform-browser-dynamic": "~4.3.1",
    "@angular/forms": "~4.3.1",
    "@angular/http": "~4.3.1",
    "@angular/router": "~4.3.1",
    "rxjs": "^5.1.0",
    "core-js": "^2.4.1",
    "systemjs": "0.19.39",
    "zone.js": "^0.8.4",
    "angular2-markdown": "^1.6.0",
    "github-markdown-css": "^2.8.0"
  },
  "devDependencies": {
    "concurrently": "^3.0.0",
    "lite-server": "^2.2.2",
    "typescript": "~2.3.2"
  },
  "repository": {}
}